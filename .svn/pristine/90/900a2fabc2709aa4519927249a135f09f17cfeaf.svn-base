#accordion {
    width: 80%;
}
#accordion div {
    background-color: #333;
    font-weight: bold;
    margin: 1px;
    cursor: pointer;
    padding: 10px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    color:#fff;
    clear: both;
    width: 100%;
}
#accordion ul {
    clear: both;
    width: 100%;
}
#accordion ul{
    display: none;
}
#accordion ul li {
    font-weight: normal;
    cursor: auto;
    background-color: #fff;
    padding: 0 0 0 7px;
}
#accordion a {
    text-decoration: none;
}
#accordion a:hover {
    text-decoration: underline;
}
